<a href="/registracion" ><img src="../imagenes/iconos/programador.png" width="75" height="75" class="mt-3"></a>

<div class="table-responsive-sm" >
<table class="table table-hover">
  <thead>
    <tr class="table-dark">
      <th scope="col">NÂº</th>
      <th scope="col">Nombre</th>
      <th scope="col">Apellido</th>
      <th scope="col">Direccion</th>
      <th scope="col">Altura</th>
      <th scope="col">Ciudad</th>
      <th scope="col">Estado</th>
      <th scope="col">CP</th>
      <th scope="col">Email</th>
      <th scope="col">Modificar</th>
    </tr>
  </thead>
  {{#each socios}} 
  <tbody>
    <tr> 
      <th scope="row">{{#masuno @index}}</th>{{/masuno}}
      <td>{{nombreRegistro}}</td>
      <td>{{apellidoRegistro}}</td>
      <td>{{calleRegistro}}</td>
      <td>{{alturaRegistro}}</td>
      <td>{{ciudadRegistro}}</td>
      <td>{{estadoRegistro}}</td>
      <td>{{cpRegistro}}</td>
      <td>{{emailRegistro}}</td>
      <td>
        <div class="d-flex justify-content-around">
       <a href="/modi/{{_id}}" class="btn btn-outline-success">Editar</a>
        <a href="/eliminarTitular/{{_id}}" class="btn btn-outline-danger">Eliminar</a>
        </div>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>


{{#if eliminar}}
<script>
Swal.fire({
  title: 'Usuario eliminado',
  text: 'La Cuenta ha sido eliminada definitivamente',
  imageUrl: "https://brandvan.io/wp-content/uploads/2021/07/giphy.gif",
  imageWidth: 400,
  imageHeight: 200,
  imageAlt: 'Usuario Eliminado',
})
</script>
{{/if}}
